select  cones.INSTITUCIÓN, cones.[CARRERA O PROGRAMA], cones.[SEDE O FILIAL], cones.[DOCUMENTO RESPALDATORIO],
  mec.nombre_institucion, mec.denominacion_carrera, mec.CONES, mec.departamento
from CONES
join MEC
on mec.denominacion_carrera like '%' + cones.[CARRERA O PROGRAMA] + '%'

WHERE
CONES.INSTITUCIÓN = mec.nombre_institucion
and 

INSTITUCIÓN like '%universidad%'

and
cones.[SEDE O FILIAL] like '%' + mec.distrito + '%'
order by nombre_institucion


