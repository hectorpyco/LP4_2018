use Carreras

select  cones.INSTITUCIÓN, mec.nombre_institucion,
 cones.denominacion_carrera, mec.denominacion_carrera, 
 cones.[SEDE O FILIAL], mec.distrito,
  mec.CONES, cones.[DOCUMENTO RESPALDATORIO]
from CONES
 join MEC
 on mec.denominacion_carrera like '%' + cones.denominacion_carrera + '%'
WHERE
CONES.INSTITUCIÓN =mec.nombre_institucion

and
cones.[SEDE O FILIAL] like '%' + mec.distrito + '%'
order by nombre_institucion


